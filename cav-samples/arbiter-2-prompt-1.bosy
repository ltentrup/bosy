{
    "semantics": "mealy",
    "inputs": ["request1", "request2"],
    "outputs": ["grant1", "grant2"],
    "assumptions": [],
    "guarantees": [
        "G (request1 -> F grant1)",
        "G (request2 -> F grant2)",
        "G !(grant1 && grant2)"
    ],
    "hyper": [
        "exists color. forall pi. GF color && GF !color && G (request1[pi] -> ( color U ( !color U grant1[pi] ) ) && ( !color U ( color U grant1[pi] ) ) )"
    ]
}